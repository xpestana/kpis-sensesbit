# Variables para Docker Compose (app + PostgreSQL)
# Copiar a .env y rellenar antes de: docker compose up -d

# PostgreSQL (contenedor postgres)
POSTGRES_USER=kpis
POSTGRES_PASSWORD=cambiar_password_seguro
POSTGRES_DB=kpis

# HubSpot (Private App - Access Token)
HUBSPOT_API_KEY=

# CORS (dominio p√∫blico de la API, ej. https://api.tudominio.com)
ORIGIN_HOSTS=https://api.tudominio.com,https://tudominio.com

# Opcional
GLOBAL_SCHEMA=shared
PROD=true
